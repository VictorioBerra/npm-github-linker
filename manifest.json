{
  "manifest_version": 2,
  "name": "GitHub NPM Linker",
  "version": "0.1",
 
  "description": "Adds links to npm packages when you visit a github package.json blob",
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" },

  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}